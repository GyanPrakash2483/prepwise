generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Roadmap {
  id                  Int    @id @default(autoincrement())
  roadmapobjectstring String
}

model User {
  id                Int      @id @default(autoincrement())
  name              String
  email             String   @unique
  credits           Int
  isverified        Boolean
  sessiontokens     String[]
  verificationtoken String?
  roadmaps          Int[]
  password          String
}
